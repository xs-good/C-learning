void Bubble_Sort(int* arr, int sz)
{
	for (int i = 0; i < sz-1; i++)
	{
		for (int j = 0; j < sz - i-1; j++)
		{
			if (*(arr + j) > *(arr + j + 1))
			{
				int temp = *(arr+j);
				*(arr+j) = *(arr+j+1);
				*(arr+j+1) = temp;
			}
		}
	}
}




void Bubble_Sort(int* arr, int sz)
{
	for (int i = 0; i < sz-1; i++)
	{
		for (int j = 0; j < sz - i-1; j++)
		{
			if (arr[i]>arr[i+1])
			{
				int temp = arr[i];
				arr[i] = arr[i+1];
				arr[i+1] = temp;
			}
		}
	}
}


