int main()
{
	unsigned char a = 128;
	//00000000 00000000 00000000 10000000
	//10000000
	//运算之前进行整型提升
	//00000000 00000000 00000000 10000000
	//进行按位取反操作
	//11111111 11111111 11111111 01111111-补码（%u输出时直接读补码）
	//10000000 00000000 00000000 10000001-原码（%d输出时需要）
	unsigned int b = (unsigned int)(~a);
	printf("%u\n%d",b,b);
	//4294967167(2^32-1-128)
	//-129
	return 0;
}
